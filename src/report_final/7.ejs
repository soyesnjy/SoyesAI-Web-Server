    <script>
      const ctx = document.getElementById("myChart1").getContext("2d");
      const myChart1 = new Chart(ctx, {
        type: "line",
        data: {
          labels: ["미션1", "미션2", "미션3", "미션4", "미션5"], // x축 레이블
          datasets: [
            {
              label: "기분 변화",
              data: <%= mood_scores %>, // 꺾은선 데이터
              borderColor: "green", // 선 색상
              borderWidth: 2,
              fill: false, // 선 아래 색상 채우기 여부
              pointBackgroundColor: "green", // 포인트 색상
              tension: 0.4, // 부드러운 곡선을 위한 tension 값 설정

              pointRadius: 10, // 포인트 크기
            },
          ],
        },
        options: {
          responsive: false, // 크기 자동 조정 비활성화
          scales: {
            x: {
              display: false, // x축 레이블 숨기기
            },
            y: {
              display: false, // y축 레이블 숨기기
              beginAtZero: true,
              max: 6, // y축 최대값 설정
              ticks: {
                stepSize: 1, // y축 간격 설정
                callback: function (value) {
                  const labels = [
                    "매우 힘들다",
                    "힘들다",
                    "보통이다",
                    "괜찮다",
                    "아주 괜찮다",
                  ];
                  return labels[value - 1]; // y축에 해당 텍스트 표시
                },
              },
            },
          },
          plugins: {
            legend: {
              display: false, // 레전드 숨기기
            },
          },
        },
      });

      const ctx2 = document.getElementById("myChart2").getContext("2d");
