              data: <%= anxiety_scores %>, // 꺾은선 데이터
              borderColor: "green", // 선 색상
              borderWidth: 2,
              fill: false, // 선 아래 색상 채우기 여부
              pointBackgroundColor: "green", // 포인트 색상
              tension: 0.4, // 부드러운 곡선을 위한 tension 값 설정

              pointRadius: 10, // 포인트 크기
            },
          ],
        },
        options: {
          responsive: false, // 크기 자동 조정 비활성화
          scales: {
            x: {
              display: false, // x축 레이블 숨기기
            },
            y: {
              display: false, // y축 레이블 숨기기
              beginAtZero: true,
              max: 6, // y축 최대값 설정
              ticks: {
