                callback: function (value) {
                  const labels = [
                    "매우 힘들다",
                    "힘들다",
                    "보통이다",
                    "괜찮다",
                    "아주 괜찮다",
                  ];
                  return labels[value - 1]; // y축에 해당 텍스트 표시
                },
              },
            },
          },
          plugins: {
            legend: {
              display: false, // 레전드 숨기기
